import pygame
pygame.init()

win = pygame.display.set_mode((700, 500))
pygame.display.set_caption("Ping-Pong")



clock = pygame.time.Clock()
Game = True
Fps = 50
while Game:

    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            Game = False

    win.fill((255, 255, 255))
    clock.tick(Fps)
    pygame.display.update()
